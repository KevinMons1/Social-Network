con icon=\"image\" className=\"icon-write-new-msg\" onClick={() => handleClickFile()} />\r\n                    <p className=\"icon-write-new-msg\">GIF</p>\r\n                </div>\r\n                <form className=\"new-msg-box\">\r\n                    <button onClick={e => handleSubmit(e)} ><FontAwesomeIcon className=\"icon-send-msg\" icon=\"paper-plane\" /></button>\r\n                    <textarea ref={messageRef} name=\"text\" onChange={e => setMessage({...message, [e.target.name]: e.target.value})} className=\"new-msg-textarea\"></textarea>\r\n                </form>\r\n            </div>\r\n        </div>\r\n    )\r\n}","D:\\Web\\Ecole\\TFE\\Application\\social-network-app\\client\\src\\Components\\ChatDiv\\Message.js",[],{"ruleId":"227","replacedBy":"228"},{"ruleId":"229","replacedBy":"230"},{"ruleId":"227","replacedBy":"231"},{"ruleId":"229","replacedBy":"232"},{"ruleId":"233","severity":1,"message":"234","line":69,"column":6,"nodeType":"235","endLine":69,"endColumn":8,"suggestions":"236"},{"ruleId":"237","severity":1,"message":"238","line":5,"column":8,"nodeType":"239","messageId":"240","endLine":5,"endColumn":16},{"ruleId":"237","severity":1,"message":"241","line":9,"column":23,"nodeType":"239","messageId":"240","endLine":9,"endColumn":35},{"ruleId":"242","severity":1,"message":"243","line":11,"column":25,"nodeType":"244","endLine":11,"endColumn":74},{"ruleId":"237","severity":1,"message":"245","line":18,"column":12,"nodeType":"239","messageId":"240","endLine":18,"endColumn":16},{"ruleId":"233","severity":1,"message":"246","line":55,"column":8,"nodeType":"235","endLine":55,"endColumn":10,"suggestions":"247"},{"ruleId":"233","severity":1,"message":"248","line":36,"column":8,"nodeType":"235","endLine":36,"endColumn":10,"suggestions":"249"},{"ruleId":"237","severity":1,"message":"250","line":1,"column":16,"nodeType":"239","messageId":"240","endLine":1,"endColumn":24},{"ruleId":"233","severity":1,"message":"251","line":74,"column":8,"nodeType":"235","endLi